{
  "name": "mateusz.im",
  "version": "1.0.0",
  "description": "Mateusz Witalinski personal website",
  "main": "gulpfile.js",
  "scripts": {
    "start": "npm-run-all build watch",
    "build": "gulp build",
    "watch": "gulp",
    "test": "echo \"Error: no test specified\" && exit 1",
    "remove-build": "rm -rf build mateusz.im mateusz.im.tar.gz",
    "build-tar": "mv build mateusz.im && tar -zcvf mateusz.im.tar.gz mateusz.im",
    "upload": "scp ./mateusz.im.tar.gz mateusz.im:~",
    "unpack-remote": "ssh mateusz.im 'rm -rf mateusz.im && tar zxf mateusz.im.tar.gz && cp mateusz.im/* /var/www/mateusz.im/ -r'",
    "deploy": "npm-run-all remove-build build build-tar upload unpack-remote remove-build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/applifactory/mateusz.im.git"
  },
  "author": "Mateusz Witalinski",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/applifactory/mateusz.im/issues"
  },
  "homepage": "https://github.com/applifactory/mateusz.im#readme",
  "dependencies": {
    "babel-preset-es2015": "^6.9.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.0",
    "gulp-connect": "^5.0.0",
    "gulp-include": "^2.3.0",
    "gulp-jade": "^1.1.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-rename": "^1.2.2",
    "gulp-stylus": "^2.5.0",
    "jquery": "^3.1.0",
    "retinajs": "^2.1.0"
  }
}
